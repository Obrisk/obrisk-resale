#Destination /etc/systemd/system/celerybeat.service
[Unit]
Description=Celery beat daemon
After=network.target

[Service]
Type=simple
User=obdev-user
Group=www-data
WorkingDirectory=/home/obdev-user/obdev2018
ExecStart=/home/obdev-user/obdev2018/venv_obrisk/bin/celery -A config beat -l CRITICAL


[Install]
WantedBy=multi-user.target
